<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ApplicationOOP.EntrancePage">
    <MasterDetailPage.Master>
        <ContentPage Title="Master">
            <ContentPage.Content>
                <StackLayout>
                    <Button x:Name="Btn_pets" Text="Registered Pets" Clicked="User_animals"/>
                    <Button x:Name="Btn_find" Text="Find owner" Clicked="Find_owner"/>
                    <Button x:Name="Btn_clinic" Text="Search clinic" Clicked="Veterinar_clinic"/>
                    <Button x:Name="Out_Btn" Text="Log out" Clicked="Log_out"/>
                </StackLayout>
            </ContentPage.Content>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>