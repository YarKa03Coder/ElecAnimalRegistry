<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ApplicationOOP.FindOwner"
             Title="Find Owner">
    <ContentPage.Content>
        <StackLayout x:Name="LayoutUser">
            <SearchBar x:Name="UserSearchBar" Placeholder="Input pet id here" SearchButtonPressed="StartFindingUser"/>
            <Frame x:Name="FrameOwner" Margin ="7" BorderColor="Gray" HasShadow="True" CornerRadius="20" VerticalOptions="CenterAndExpand">
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>